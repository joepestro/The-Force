<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">

		<em:id>theforce@joe.pestro</em:id>
		<em:name>The Force</em:name>
		<em:version>0.1</em:version>
		<em:description>Binds your Rails project to Firefox.</em:description>
		<em:creator>Joe Pestro</em:creator>
		<em:homepageURL>http://joepestro.github.com</em:homepageURL>
		<em:iconURL>chrome://theforce/skin/about.png</em:iconURL>

		<em:file>
			<Description about="urn:mozilla:extension:file:theforce.jar">
				<em:package>content/</em:package>
				<em:skin>skin/classic/</em:skin>
				<em:locale>locale/en-US/</em:locale>
			</Description>
		</em:file>


		<!-- Firefox -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>2.0</em:minVersion>
				<em:maxVersion>3.5.*</em:maxVersion>
			</Description>
		</em:targetApplication>

	</Description>

</RDF>

